{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "SteamApi": {
    "ApiKey": "",
    "AllGamesUrl": "https://api.steampowered.com/IStoreService/GetAppList/v1/?key={API-KEY}&max_results=50000",
    "GameAllInfo": "https://store.steampowered.com/api/appdetails?appids={APPID}&cc={CC}&l=english",
    "GamePriceInfo": "https://store.steampowered.com/api/appdetails?appids={APPID}&cc={CC}&filters=price_overview",
    "WishlistUrl": "https://api.steampowered.com/IWishlistService/GetWishlist/v1/?key={API-KEY}&steamid={STEAMID}",
    "CountryCodes": ["us","ae", "au", "br", "ca", "ch", "cl", "cn", "co", "cr", "cz", "gb", "il", "id", "in", "jp", "kr", "kw", "kz", "mx", "my", "no", "nz", "pe", "ph", "pl", "qa", "ru", "sa", "sg", "th", "tw", "ua", "ar", "dz", "np", "am", "uy", "vn", "za"]
  },
  "DataStorage": {
    "FolderPath": "startupData",
    "LoadDataFromFileOnStartup": false
  },
  "CurrencyApi": {
    "BaseUrl": "https://v6.exchangerate-api.com/v6/{API-KEY}/latest/",
    "ApiKey": ""
  },
  "Wishlists": {
    "PageSize": 5
  }
}
